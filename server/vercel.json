{
  "version": 2,
  "builds": [
      {
          "src": "sever.js",
          "use": "@vercel/node",
          "config": {
              "includeFiles": [
                  "dist/**"
              ]
          }
      }
  ],
  "routes": [
      {
          "src": "/(.*)",
          "dest": "sever.js"
      }
  ]
}