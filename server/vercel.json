{
    "version": 2,
    "builds": [
      {
        "src": "sever.js",
        "use": "@vercel/node",
        "config": {
          "includeFiles": ["dist/**"],
          "maxDuration": 30
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "sever.js"
      }
    ]
  }
  